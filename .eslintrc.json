{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": ["jest"],
    "rules": {
        "semi": 1,
        "no-console": 1,
        "no-underscore-dangle": 0,
        "class-methods-use-this": 0,
        "no-undef": 1,
        "no-unused-vars": 1
    }
}
